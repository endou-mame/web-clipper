{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "web-clipper-api",
  "main": "src/index.ts",
  "compatibility_date": "2025-12-01",
  "compatibility_flags": ["nodejs_compat"],
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "web-clipper-db",
      "database_id": "<DATABASE_ID>"
    }
  ]
}
